{
	"name": "fixture/test-extension",
	"type": "typo3-cms-extension",
	"description": "This is just a test extension created by the ExtensionBuilder",
	"authors": [
		{
			"name": "John Doe",
			"role": "Developer"
		}
	],
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": "^10.4"
	},
	"require-dev": {
		"typo3/testing-framework": "^6.8"
	},
	"autoload": {
		"psr-4": {
			"FIXTURE\\TestExtension\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FIXTURE\\TestExtension\\Tests\\": "Tests"
		}
	},
	"replace": {
		"typo3-ter/test-extension": "self.version"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"scripts": {
		"post-autoload-dump": [
			"TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
		]
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/public",
			"extension-key": "test_extension"
		}
	}
}
